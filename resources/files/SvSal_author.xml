                                <valItem ident="image"/>
                                <valItem ident="note-anchor"/>
                                <valItem ident="url"/>
                                <valItem ident="summary"/>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                <elementSpec ident="settlement" module="namesdates" mode="change"> <!-- not € att.datable, att.naming, att.typed -->
                    <classes mode="change">
