                        <memberOf key="att.typed" mode="delete"/>
                    </classes>
                </elementSpec>
                <elementSpec ident="supplied" module="transcr" mode="change">    <!-- define possible @reasons, @resp and @cert required -->
                    <attList>
                        <attDef ident="cert" mode="change" usage="req"/>
                        <attDef ident="reason" mode="change" usage="req">
                            <valList type="closed" mode="replace">
                                <valItem ident="damage"/>
                                <valItem ident="deletion"/>
                            </valList>
                        </attDef>
                        <attDef ident="resp" mode="change" usage="req"/>
                    </attList>
                </elementSpec>
                <elementSpec ident="term" module="core" mode="change">       <!-- xml:id required -->
                    <attList>
                        <attDef ident="xml:id" mode="change" usage="req"/>
                    </attList>
                </elementSpec>
                <elementSpec ident="text" module="textstructure" mode="change"> <!-- define possible @type (author_article) and @xml:ids -->
                    <attList>
                        <attDef ident="type" mode="change" usage="req">
                            <valList type="closed" mode="replace">
                                <valItem ident="author_article"/>
                            </valList>
                        </attDef>
                        <attDef ident="xml:id" mode="change" usage="req">
                            <valList type="closed" mode="replace">
                                <valItem ident="completeWork"/>
                                <valItem ident="vol01"/>
                                <valItem ident="vol02"/>
                                <valItem ident="vol03"/>
                                <valItem ident="vol04"/>
                                <valItem ident="vol05"/>
                                <valItem ident="vol06"/>
                                <valItem ident="vol07"/>
                                <valItem ident="vol08"/>
                                <valItem ident="vol09"/>
                                <valItem ident="vol10"/>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                <elementSpec ident="title" module="core" mode="change">       <!-- changed @types, @key and @ref -->
                    <attList>
                        <attDef ident="type" mode="change" usage="opt">
                            <desc>Only to use in the TEI-header. (classifies the title according to
                      some convenient typology).</desc>
                            <desc>Use: required in the TEI-header. (classifies the title according
                      to some convenient typology).</desc>
                            <valList type="closed" mode="replace">
                                <valItem ident="main"/>
                                <valItem ident="short"/>
                                <valItem ident="sub"/>
                                <valItem ident="volume"/>
                            </valList>
                        </attDef>
                        <attDef ident="ref" mode="change">
                            <desc>Only to use in the body. provides an externally-defined means of
                      identifying the entity (or entities) being named, using an
                      URI.</desc>
                        </attDef>
                        <attDef ident="key" mode="change">
                            <desc>Only to use in the body. provides an externally-defined means of
                      identifying the entity (or entities) being named, using a coded
                      value of some kind.</desc>
                        </attDef>
                    </attList>
                </elementSpec>
                <elementSpec ident="include" ns="http://www.w3.org/2001/XInclude" mode="add">
                    <desc xml:lang="en" versionDate="2014-01-12">The W3C XInclude element</desc>
                    <classes>
                        <memberOf key="model.common"/>
                        <memberOf key="model.headerPart"/>
                    </classes>
                    <content>
                        <rng:optional>
                            <rng:ref name="fallback"/>
                        </rng:optional>
                    </content>
                    <attList>
                        <attDef ident="href">
                            <desc xml:lang="en" versionDate="2014-01-12">pointer to the resource being included</desc>
                            <datatype>
                                <rng:ref name="data.pointer"/>
                            </datatype>
                        </attDef>
                        <attDef ident="parse" usage="opt">
                            <defaultVal>xml</defaultVal>
                            <valList type="closed">
                                <valItem ident="xml"/>
                                <valItem ident="text"/>
                            </valList>
                        </attDef>
                        <attDef ident="xpointer" usage="opt">
                            <datatype>
                                <rng:text/>
                            </datatype>
                        </attDef>
                        <attDef ident="encoding" usage="opt">
                            <datatype>
                                <rng:text/>
                            </datatype>
                        </attDef>
                        <attDef ident="accept" usage="opt">
                            <datatype>
                                <rng:text/>
                            </datatype>
                        </attDef>
                        <attDef ident="accept-charset" usage="opt">
                            <datatype>
                                <rng:text/>
                            </datatype>
                        </attDef>
                        <attDef ident="accept-language" usage="opt">
                            <datatype>
                                <rng:text/>
                            </datatype>
                        </attDef>
                    </attList>
                </elementSpec>
                <elementSpec ident="fallback" ns="http://www.w3.org/2001/XInclude" mode="add">
                    <desc xml:lang="en" versionDate="2014-01-12">Wrapper for fallback elements if an XInclude fails</desc>
                    <content>
                        <oneOrMore xmlns="http://relaxng.org/ns/structure/1.0">
                            <choice>
                                <text/>
                                <ref name="macro.anyThing"/>
                            </choice>
                        </oneOrMore>
                    </content>
                </elementSpec>
            </schemaSpec>
        </body>
