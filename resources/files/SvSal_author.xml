                <elementSpec ident="placeName" module="namesdates" mode="change"> <!-- not € att.datable, att.editLike -->
                    <classes mode="change">
                        <memberOf key="att.datable" mode="delete"/>
                        <memberOf key="att.editLike" mode="delete"/>
                        <memberOf key="att.dimensions" mode="delete"/>
                    </classes>
                </elementSpec>
                <elementSpec ident="pubPlace" module="core" mode="change">       <!-- define possible @roles (required) -->
                    <attList>
                        <attDef ident="role" mode="change" usage="req">
                            <datatype minOccurs="1" maxOccurs="2">
                                <rng:text/>
                            </datatype>
                            <defaultVal>firstEd,thisEd</defaultVal>
                            <valList type="closed" mode="replace">
                                <valItem ident="firstEd"/>
                                <valItem ident="thisEd"/>
