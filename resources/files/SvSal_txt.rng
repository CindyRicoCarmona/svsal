                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      context="tei:list[@type='gloss']">
	              <sch:assert test="tei:label">The content of a "gloss" list should include a sequence of one or more pairs of a label element followed by an item element</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="att.global.attribute.rend"/>
         <ref name="att.global.attribute.style"/>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.linking.attribute.corresp"/>
         <ref name="att.global.linking.attribute.sameAs"/>
         <ref name="att.global.linking.attribute.copyOf"/>
         <ref name="att.global.linking.attribute.next"/>
         <ref name="att.global.linking.attribute.prev"/>
         <ref name="att.global.linking.attribute.exclude"/>
         <ref name="att.global.analytic.attribute.ana"/>
         <ref name="att.global.facs.attribute.facs"/>
         <ref name="att.global.responsibility.attribute.cert"/>
         <ref name="att.global.responsibility.attribute.resp"/>
         <ref name="att.sortable.attributes"/>
         <ref name="att.typed.attribute.subtype"/>
         <optional>
            <attribute name="rendition">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">points to a description of the rendering or presentation used for this element in the source text.</a:documentation>
               <list>
                  <oneOrMore>
                     <choice>
                        <value>#leftBraced</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(grouping brace, pointing to the left) </a:documentation>
                        <value>#rightBraced</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(grouping brace, pointing to the right) </a:documentation>
                        <value>#topBraced</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(grouping brace, pointing upwards) </a:documentation>
                        <value>#bottomBraced</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(grouping brace, pointing downwards) </a:documentation>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="type"
                       a:defaultValue="simple">
               <a:documentation>describes the nature of the items in the list.</a:documentation>
               <choice>
                  <value>bulleted</value>
                  <a:documentation/>
                  <value>dict</value>
                  <a:documentation/>
                  <value>gloss</value>
                  <a:documentation/>
                  <value>ordered</value>
                  <a:documentation/>
                  <value>simple</value>
                  <a:documentation/>
                  <value>summaries</value>
                  <a:documentation/>
                  <value>index</value>
                  <a:documentation/>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
