                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(superscript) </a:documentation>
                        <value>#sub</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(subscript) </a:documentation>
                        <value>#initCaps</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Initials and initial capitals) </a:documentation>
                        <value>#r-center</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(horizontally centered) </a:documentation>
                        <value>#l-indent</value>
