   <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="SvSal_author-att.spanning-spanTo-spanTo-2-constraint-3">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:rng="http://relaxng.org/ns/structure/1.0" context="tei:*[@spanTo]">
            <sch:assert test="id(substring(@spanTo,2)) and following::*[@xml:id=substring(current()/@spanTo,2)]">
